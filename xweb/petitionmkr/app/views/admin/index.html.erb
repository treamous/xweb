<h1>Welcome</h1>
It's <%= Time.now %>

<% if flash[:notice] %>
  <%= h flash[:notice] %>
<% end %>
 
<% form_for :user, @user do |f| %>
  <%= error_messages_for :user %>
 
  Username: <%= f.text_field :username %><br/>
  Password: <%= f.password_field :password %><br/>
  <%= submit_tag &quot;Login&quot; %>
<% end %>